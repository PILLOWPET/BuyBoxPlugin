<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <import resource="services/payum.xml"/>
    </imports>

    <services>
        <service id="sylius.paypal.client.sftp" class="phpseclib\Net\SFTP">
            <argument>%sylius.pay_pal.reports_sftp_host%</argument>
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Processor\PayPalAddressProcessor">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
    </services>
</container>
