<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <import resource="services/api.xml"/>
        <import resource="services/onboarding.xml"/>
        <import resource="services/payum.xml"/>
    </imports>

    <services>
        <service id="Onatera\SyliusBuyboxPlugin\Form\Extension\PaymentMethodTypeExtension">
            <tag name="form.type_extension" extended-type="Sylius\Bundle\PaymentBundle\Form\Type\PaymentMethodType" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Generator\PayPalAuthAssertionGeneratorInterface"
            class="Onatera\SyliusBuyboxPlugin\Generator\PayPalAuthAssertionGenerator"
        />

        <service id="Onatera\SyliusBuyboxPlugin\Listener\PayPalPaymentMethodListener">
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Onboarding\Initiator\OnboardingInitiatorInterface" />
            <argument type="service" id="router" />
            <argument type="service" id="session.flash_bag" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Provider\PayPalPaymentMethodProviderInterface" />
            <tag name="kernel.event_listener" event="sylius.payment_method.initialize_create" method="initializeCreate" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Manager\PaymentStateManagerInterface"
            class="Onatera\SyliusBuyboxPlugin\Manager\PaymentStateManager"
        >
            <argument type="service" id="sm.factory" />
            <argument type="service" id="sylius.manager.payment" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Processor\PaymentCompleteProcessorInterface" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Factory\PayPalPaymentMethodNewResourceFactory" decorates="sylius.resource_controller.new_resource_factory">
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Factory\PayPalPaymentMethodNewResourceFactory.inner" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Onboarding\Processor\OnboardingProcessorInterface" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Provider\OrderProviderInterface" class="Onatera\SyliusBuyboxPlugin\Provider\OrderProvider">
            <argument type="service" id="sylius.repository.order" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PaymentProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PaymentProvider"
        >
            <argument type="service" id="sylius.repository.payment" />
            <argument type="service" id="sylius.repository.order" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\OrderItemNonNeutralTaxProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\OrderItemNonNeutralTaxesProvider"
        />

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PayPalItemDataProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PayPalItemDataProvider"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Provider\OrderItemNonNeutralTaxProviderInterface" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PayPalPaymentMethodProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PayPalPaymentMethodProvider"
        >
            <argument type="service" id="sylius.repository.payment_method" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PayPalRefundDataProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PayPalRefundDataProvider"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\CacheAuthorizeClientApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\GenericApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Provider\PayPalPaymentMethodProviderInterface" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PayPalOrderProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PayPalOrderProvider"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Provider\PaymentProviderInterface" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Resolver\CapturePaymentResolverInterface" class="Onatera\SyliusBuyboxPlugin\Resolver\CapturePaymentResolver">
            <argument type="service" id="payum" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Resolver\CompleteOrderPaymentResolverInterface" class="Onatera\SyliusBuyboxPlugin\Resolver\CompleteOrderPaymentResolver">
            <argument type="service" id="payum" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\AfterCheckoutOrderPaymentProcessor"
            decorates="sylius.order_processing.order_payment_processor.after_checkout"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Processor\AfterCheckoutOrderPaymentProcessor.inner" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\OrderPaymentProcessor"
            decorates="sylius.order_processing.order_payment_processor.checkout"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Processor\OrderPaymentProcessor.inner" />
            <argument type="service" id="sm.factory" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Processor\PayPalOrderCompleteProcessor" public="true">
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Manager\PaymentStateManagerInterface" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\PaymentCompleteProcessorInterface"
            class="Onatera\SyliusBuyboxPlugin\Processor\PayPalPaymentCompleteProcessor"
        >
            <argument type="service" id="payum" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\LocaleProcessorInterface"
            class="Onatera\SyliusBuyboxPlugin\Processor\LocaleProcessor"
        />

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\PaymentRefundProcessorInterface"
            class="Onatera\SyliusBuyboxPlugin\Processor\PayPalPaymentRefundProcessor"
            public="true"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\CacheAuthorizeClientApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\OrderDetailsApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\RefundPaymentApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Generator\PayPalAuthAssertionGeneratorInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Provider\RefundReferenceNumberProviderInterface" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Processor\UiPayPalPaymentRefundProcessor"
            decorates="Onatera\SyliusBuyboxPlugin\Processor\PaymentRefundProcessorInterface"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Processor\UiPayPalPaymentRefundProcessor.inner" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PayPalConfigurationProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PayPalConfigurationProvider"
        >
            <argument type="service" id="sylius.repository.payment_method" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\UuidProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\UuidProvider"
        />

        <service id="sylius.paypal.client.sftp" class="phpseclib\Net\SFTP">
            <argument>%sylius.pay_pal.reports_sftp_host%</argument>
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Downloader\ReportDownloaderInterface"
            class="Onatera\SyliusBuyboxPlugin\Downloader\SftpPayoutsReportDownloader"
        >
            <argument type="service" id="sylius.paypal.client.sftp" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Processor\PayPalAddressProcessor">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Updater\PaymentUpdaterInterface"
            class="Onatera\SyliusBuyboxPlugin\Updater\PayPalPaymentUpdater"
        >
            <argument type="service" id="sylius.manager.payment" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Enabler\PaymentMethodEnablerInterface"
            class="Onatera\SyliusBuyboxPlugin\Enabler\PayPalPaymentMethodEnabler"
        >
            <argument type="service" id="sylius.http_client" />
            <argument>%sylius.pay_pal.facilitator_url%</argument>
            <argument type="service" id="sylius.manager.payment_method" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Registrar\SellerWebhookRegistrarInterface" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\PaymentReferenceNumberProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\PaymentReferenceNumberProvider"
        />

        <service id="Onatera\SyliusBuyboxPlugin\Command\CompletePaidPaymentsCommand">
            <argument type="service" id="sylius.repository.payment" />
            <argument type="service" id="sylius.manager.payment" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\CacheAuthorizeClientApiInterface" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\OrderDetailsApiInterface" />
            <argument type="service" id="sm.factory" />
            <tag name="console.command" command="sylius:pay-pal-plugin:complete-payments" />
        </service>

        <service
            id="Onatera\SyliusBuyboxPlugin\Provider\RefundReferenceNumberProviderInterface"
            class="Onatera\SyliusBuyboxPlugin\Provider\RefundReferenceNumberProvider"
        />

        <service
            id="Onatera\SyliusBuyboxPlugin\Registrar\SellerWebhookRegistrarInterface"
            class="Onatera\SyliusBuyboxPlugin\Registrar\SellerWebhookRegistrar"
        >
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\AuthorizeClientApiInterface" />
            <argument type="service" id="router" />
            <argument type="service" id="Onatera\SyliusBuyboxPlugin\Api\WebhookApiInterface" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Twig\PayPalExtension">
            <tag name="twig.extension" />
        </service>

        <service id="Onatera\SyliusBuyboxPlugin\Twig\OrderAddressExtension">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
